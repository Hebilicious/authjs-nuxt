{
  "name": "@hebilicious/authjs-nuxt",
  "type": "module",
  "version": "0.3.0-beta.2",
  "private": true,
  "packageManager": "pnpm@8.6.12",
  "scripts": {
    "dev": "pnpm -r --filter=./packages/* run dev",
    "build": "rimraf packages/*/dist && turbo run build --filter=@hebilicious/*",
    "build:docs": "turbo run build --filter=authjs_docs",
    "build:all": "turbo run build",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "pnpm lint:fix && bumpp -r -x \"pnpm run changelog\" --all",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:all": "pnpm lint && pnpm typecheck && pnpm vitest run"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.5",
    "@hebilicious/eslint-config": "0.0.3-beta.2",
    "@nuxt/test-utils": "^3.6.5",
    "@types/node": "^20.4.8",
    "bumpp": "^9.1.1",
    "conventional-changelog-cli": "^3.0.0",
    "eslint": "8.46.0",
    "esno": "^0.17.0",
    "lint-staged": "^13.2.3",
    "pnpm": "8.6.12",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "tsup": "^7.2.0",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vitest": "^0.34.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --cache --fix"
    ]
  }
}
